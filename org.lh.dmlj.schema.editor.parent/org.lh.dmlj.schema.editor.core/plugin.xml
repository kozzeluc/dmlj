         <propertyTab
               afterTab="org.lh.dmlj.schema.editor.propertyTabDiagram"
               category="schema"
               id="org.lh.dmlj.schema.editor.propertyTabSchemaSyntax"
               label="Syntax">
         </propertyTab>
      </propertyTabs>
   </extension>
