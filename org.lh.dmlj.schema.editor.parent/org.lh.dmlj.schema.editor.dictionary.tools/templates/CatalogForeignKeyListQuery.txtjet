<%@ jet imports="org.lh.dmlj.schema.editor.dictionary.tools.table.*"
        package="org.lh.dmlj.schema.editor.dictionary.tools.template" 
        class="CatalogForeignKeyListQueryTemplate"
        skeleton="query-template.skeleton"%>
SELECT <%= Constkey_1030.COLUMNS %> 
FROM SYSTEM.CONSTKEY
WHERE CONSTKEY.SCHEMA = 'SYSTEM' AND
      CONSTKEY.CONSTRAINT = CONSTRAINT.NAME
ORDER BY CONSTKEY.NAME,
		 CONSTKEY.SEQUENCE